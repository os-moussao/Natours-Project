  "bugs": {
    "url": "https://github.com/Oussama-Moussaoui/Natours-Project/issues"
  },
  "homepage": "https://github.com/Oussama-Moussaoui/Natours-Project#readme",
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "concat": "^1.0.3",
    "node-sass": "^5.0.0",
    "postcss-cli": "^8.3.1"
  }
}
